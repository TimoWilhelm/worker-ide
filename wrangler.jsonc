/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "worker-ide",
	"main": "src/index.ts",
	"compatibility_date": "2026-01-31",
	"compatibility_flags": ["nodejs_compat"],
	"durable_objects": {
		"bindings": [
			{
				"name": "DO_FILESYSTEM",
				"class_name": "DurableObjectFilesystem",
			},
			{
				"name": "DO_HMR_COORDINATOR",
				"class_name": "HMRCoordinator",
			},
		],
	},
	"migrations": [
		{
			"new_sqlite_classes": ["DurableObjectFilesystem", "HMRCoordinator"],
			"tag": "v2",
		},
	],

	"assets": {
		"directory": "./public",
		"binding": "ASSETS",
		"not_found_handling": "single-page-application",
		"run_worker_first": ["/api/*", "/p/*", "/about"],
	},
	"observability": {
		"logs": { "enabled": true },
		"traces": { "enabled": true },
	},
	"worker_loaders": [
		{
			"binding": "LOADER",
		},
	],
	"ratelimits": [
		{
			"name": "REPLICATE_RATE_LIMITER",
			"namespace_id": "1001",
			"simple": {
				"limit": 300,
				"period": 60,
			},
		},
	],
}
