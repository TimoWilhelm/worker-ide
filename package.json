{
	"name": "worker-ide",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"packageManager": "bun@1.3.9",
	"scripts": {
		"prepare": "husky",
		"dev": "bun run build:worker-types && concurrently -n vite,types -c blue,yellow \"vite --host 0.0.0.0 --port 3000\" \"tsc -p tsconfig.worker.json --watch --preserveWatchOutput\"",
		"build": "turbo run build:vite",
		"build:worker-types": "tsc -p tsconfig.worker.json",
		"build:vite": "vite build",
		"typecheck": "turbo run typecheck:app typecheck:node typecheck:worker",
		"typecheck:app": "tsc --noEmit -p tsconfig.app.json",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json",
		"typecheck:worker": "tsc --noEmit -p tsconfig.worker.json",
		"lint": "prettier --check . && eslint --cache --format=stylish .",
		"format": "prettier --write . && eslint --cache --format=stylish --fix .",
		"test": "vitest",
		"test:unit": "vitest --project unit",
		"test:worker": "vitest --project worker",
		"test:react": "vitest --project react",
		"test:e2e": "playwright test",
		"test:e2e:report": "playwright show-report",
		"preview": "turbo run build:vite && vite preview --host 0.0.0.0 --port 4173",
		"deploy": "turbo run build:vite && wrangler deploy",
		"cf-typegen": "wrangler types",
		"knip": "knip",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.0.0",
		"@cloudflare/vitest-pool-workers": "^0.12.13",
		"@codemirror/autocomplete": "^6.18.6",
		"@codemirror/commands": "^6.8.1",
		"@codemirror/lang-css": "^6.3.1",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.2.4",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/language": "^6.11.0",
		"@codemirror/lint": "^6.8.5",
		"@codemirror/search": "^6.5.10",
		"@codemirror/state": "^6.5.2",
		"@codemirror/view": "^6.36.8",
		"@eslint/js": "^9.39.2",
		"@hono/zod-validator": "^0.7.6",
		"@lezer/highlight": "^1.2.3",
		"@modelcontextprotocol/sdk": "^1.26.0",
		"@playwright/test": "^1.52.0",
		"@storybook/addon-essentials": "^8.6.12",
		"@storybook/react": "^8.6.12",
		"@storybook/react-vite": "^8.6.12",
		"@storybook/test": "^8.6.12",
		"@tailwindcss/vite": "^4.1.7",
		"@tanstack/react-query": "^5.75.7",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^22.0.0",
		"@types/react": "^19.1.4",
		"@types/react-dom": "^19.1.5",
		"@vitejs/plugin-react": "^4.5.2",
		"chobitsu": "^1.8.6",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"codemirror": "^6.0.1",
		"concurrently": "^9.2.1",
		"diff": "^8.0.3",
		"durable-object-fs": "^0.1.1",
		"esbuild-wasm": "^0.27.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-better-tailwindcss": "^4.0.0-prettier.1",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-react-compiler": "^19.1.0-rc.2",
		"eslint-plugin-react-hooks": "^7.0.0",
		"eslint-plugin-react-refresh": "^0.4.26",
		"eslint-plugin-storybook": "^10.1.10",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"globals": "^16.2.0",
		"hono": "^4.7.10",
		"husky": "^9.1.7",
		"isomorphic-git": "^1.37.1",
		"jsdom": "^26.1.0",
		"knip": "^5.83.1",
		"lint-staged": "^16.2.7",
		"lucide-react": "^0.563.0",
		"minimatch": "^10.2.1",
		"playwright": "^1.52.0",
		"prettier": "^3.8.1",
		"radix-ui": "^1.4.3",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-markdown": "^10.1.0",
		"react-resizable-panels": "^4.6.2",
		"remark-gfm": "^4.0.1",
		"replicate": "^1.4.0",
		"storybook": "^8.6.12",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.7",
		"turbo": "^2.8.7",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.33.1",
		"vite": "^6.0.0",
		"vitest": "^3.1.4",
		"worker-fs-mount": "^0.1.1",
		"wrangler": "^4.62.0",
		"zod": "^4.2.1",
		"zustand": "^5.0.5"
	},
	"overrides": {
		"worker-fs-mount": "^0.1.1",
		"zod-validation-error": "^5.0.0",
		"@vitest/expect": "3.2.4",
		"@vitest/spy": "3.2.4"
	}
}
