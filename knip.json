{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"ignoreExportsUsedInFile": {
		"interface": true,
		"type": true
	},
	"exclude": ["types"],
	"entry": ["worker/index.ts", "auxiliary/biome/index.ts"],
	"project": ["src/**", "worker/**", "auxiliary/**", "shared/**", "test/**"],
	"ignoreDependencies": ["cloudflare", "codemirror", "@standard-schema/spec"],
	"ignoreFiles": [
		"src/vite-env.d.ts",
		"playwright.config.ts",
		"shared/index.ts",
		"shared/errors.ts",
		"shared/ws-messages.ts",
		"src/components/index.ts",
		"src/components/ui/index.ts",
		"worker/lib/index.ts",
		"worker/services/index.ts",
		"worker/lib/content-type.ts",
		"worker/lib/preview-scripts/*.js",
		"src/features/ai-assistant/components/changed-files-summary.tsx",
		"src/features/ai-assistant/hooks/use-change-review.ts"
	]
}
